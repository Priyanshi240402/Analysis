{
  "version": 2,
  "builds": [
    {
      "src": "youtube.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/fetch_and_store/(.*)",
      "dest": "/youtube.py",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "/youtube.py"
    }
  ]
}
